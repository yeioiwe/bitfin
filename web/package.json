{
    "name": "web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "codegen": "orval && openapi-zod-client http://127.0.0.1:8080/api/v1-json -t ./shared/config/api/api.hbs -o ./shared/config/api/api.zod.ts",
        "dev": "npm run codegen && tsc && PORT=3005 next dev",
        "build": "tsc && NODE_ENV=production next build",
        "start": "PORT=3005 next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@emotion/react": "^11.13.5",
        "@emotion/styled": "^11.13.5",
        "@mui/icons-material": "^6.1.9",
        "@mui/material": "^6.1.9",
        "@tanstack/react-query": "^4.36.1",
        "axios": "^1.7.9",
        "next": "^15.0.3",
        "openapi-zod-client": "^1.18.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.53.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "orval": "^7.3.0",
        "prettier": "3.4.1",
        "typescript": "^5"
    }
}
